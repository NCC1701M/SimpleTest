{
	"dockerComposeFile": "docker-compose.yml",
	"service": "app",
	"workspaceFolder": "/workspaces/SimpleTest",
	"remoteEnv": {
		"LOCAL_WORKSPACE_FOLDER": "${localWorkspaceFolder}",
		"FULL_NAME": "${localEnv:FULL_NAME}",
		"USER_MAIL": "${localEnv:USER_MAIL}",
		"TOKEN_SEQ_SIMPLETEST": "${localEnv:TOKEN_SEQ_SIMPLETEST}"
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"eamodio.gitlens",
				"quicktype.quicktype",
				"mtxr.sqltools",
				"dotjoshjohnson.xml",
				"gruntfuggly.todo-tree",
				"heaths.vscode-guid",
				"mtxr.sqltools-driver-mssql",
				"hookyqr.beautify",
				"dbaeumer.vscode-eslint",
				"k--kato.docomment",
				"ms-dotnettools.csharp",
				"ms-dotnettools.csdevkit",
				"ms-dotnettools.vscode-dotnet-runtime"
			],
			"settings": {
				"sqltools.connections": [
					{
						"mssqlOptions": {
							"appName": "SQLTools",
							"useUTC": true,
							"encrypt": true
						},
						"previewLimit": 50,
						"server": "localhost",
						"port": 1433,
						"driver": "MSSQL",
						"name": "local",
						"database": "master",
						"username": "sa",
						"password": "APr3ttyStr0ngP@ssword"
					}
				],
				"dotnet.defaultSolution": "SimpleTest.sln"
			}
		}
	},
	"forwardPorts": [
		1433
	],
	"portsAttributes": {
		"1433": {
			"label": "Database",
			"onAutoForward": "silent"
		}
	},
	"postCreateCommand": "/bin/bash /workspaces/SimpleTest/.devcontainer/custom-scripts/setup.sh",
	"name": "SimpleTest"
}