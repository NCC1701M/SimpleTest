{
	"version": "0.2.0",
	"configurations": [
		{
			"name": ".NET Core Launch (web)",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "Build Server",
			"program": "${workspaceFolder}/SimpleTest/bin/Debug/SimpleTest.dll",
			"args": [],
			"cwd": "${workspaceFolder}/SimpleTest",
			"stopAtEntry": false,
			"env": {
				"ASPNETCORE_ENVIRONMENT": "Development"
			},
			"sourceFileMap": {
				"/Views": "${workspaceFolder}/Views"
			}
		}
	]
}